<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ool.lookting"
    android:versionCode="1"
    android:versionName="0.1" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lookting_icon"
        android:label="@string/app_name"
        android:theme="@style/Theme.Example" >
        <activity
            android:name="com.ool.lookting.TingsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ool.lookting.TingActivity"
            android:parentActivityName="com.ool.lookting.TingsActivity"
            android:label="@string/title_activity_ting" >
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="om.ool.lookting.TingsActivity" />
        </activity>
    </application>

</manifest>
